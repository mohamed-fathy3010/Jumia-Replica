@{ 
    Layout = "~/Views/Shared/Seller_layout.cshtml";
    ViewBag.title= "Feedback";

}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    @section styles{
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/Content/css/Feedback.css" rel="stylesheet" />
    <link rel="icon" type="image/ico" href="https://www.jumia.com.eg/assets_he/favicon.717283be.ico">
}
</head>
<body>
    <div id="main" class="container-fluid">
        <section class="feedbackController">
            <div class="page-title">
                <h4>Feedback
                    <p>
                        <span>Take a tour</span>
                    </p>
                </h4>
            </div>

            <div class="card-container">
                    <div class="row row-cols-1 row-cols-md-3 g-4">

                        <div class="col-12 col-sm-4 cardss">
                          <div class="card h-100">
                            <div class="card-body">
                              <h5 class="card-title">Last 30 days</h5>
                              <div class="count">0%</div>
                            </div>
                          </div>
                        </div>

                        <div class="col-12 col-sm-4 cardss">
                          <div class="card h-100">
                            <div class="card-body">
                              <h5 class="card-title">Last 180 days</h5>
                              <div class="count">0%</div>
                            </div>
                          </div>
                        </div>

                        <div class="col-12 col-sm-4 cardss">
                          <div class="card h-100">
                            <div class="card-body">
                              <h5 class="card-title">Last 365 days</h5>
                              <div class="count">0%</div>
                            </div>
                          </div>
                        </div>

                      </div>
            </div>

            <div class="feedback-section">
              <div class="feedback-area">
                <div class="feedback-upper">
                    <div>
                      <table class="table">
                        <thead>
                          <tr>                        
                            <th colspan="4" id="caption">Feedback Rating Summary</th>
                          </tr>
                          <tr>
                            <th>Rating</th>
                            <th>Last 30 days</th>
                            <th>Last 180 days</th>
                            <th>Last 365 days</th>
                          </tr>
                        </thead>
                        <tbody>
                          <tr>
                            <td><i class="fa fa-plus-circle"></i> Positive</td>
                            <td>0</td>
                            <td>0</td>
                            <td>0</td>
                          </tr>
                          <tr>
                            <td><i class="fa fa-minus-circle"></i> Negative</td>
                            <td>0</td>
                            <td>0</td>
                            <td>0</td>
                          </tr>
                        </tbody>
                        <tfoot>
                          <tr>
                            <td>Total</td>
                            <td>0</td>
                            <td>0</td>
                            <td>0</td>
                          </tr>
                        </tfoot>
                      </table>
                  </div>
                </div>
              </div>
            </div>

            <div class="feedBack-details">
              <div class="details-area">
                <div class="details-upper">
                  <div>
                    <table class="table">
                      <thead>
                        <tr>                        
                          <th id="caption">Feedback Details</th>
                        </tr>
                        <tr>
                          <th>
                            <div class="row" style="justify-content: space-between;">
                              <div class="col-12 col-md-8 ">
                                <ul class="nav nav-pills mb-3" id="pills-tab" role="tablist">
                                  <li class="nav-item" role="presentation">
                                    <button class="nav-link active" id="pills-home-tab" data-bs-toggle="pill" data-bs-target="#pills-home" type="button" role="tab" aria-controls="pills-home" aria-selected="true">ALL</button>
                                  </li>
                                  <li class="nav-item" role="presentation">
                                    <button class="nav-link" id="pills-profile-tab" data-bs-toggle="pill" data-bs-target="#pills-profile" type="button" role="tab" aria-controls="pills-profile" aria-selected="false">Postive</button>
                                  </li>
                                  <li class="nav-item" role="presentation">
                                    <button class="nav-link" id="pills-contact-tab" data-bs-toggle="pill" data-bs-target="#pills-contact" type="button" role="tab" aria-controls="pills-contact" aria-selected="false">Negative</button>
                                  </li>
                                </ul>
                              </div>
                              <div class="d-none d-md-block col-md-4 tab-content" id="pills-tabContent">
                                <div class="right" style="margin-right: 1rem;">
                                    <!-- <div class="right-left"> -->
                                      <span>Results per page &nbsp;</span>
                                      <select ng-model="options.size" ng-change="updatePagination(options.size)" ng-options="dLimit as dLimit for dLimit in paginationLimits" class="ng-pristine ng-untouched ng-valid">
                                        <option label="10" value="number:10">10</option>
                                        <option label="20" value="number:20" selected="selected">20</option>
                                        <option label="30" value="number:30">30</option>
                                        <option label="40" value="number:40">40</option>
                                        <option label="50" value="number:50">50</option>
                                      </select>
                                    <!-- </div> -->
                                    <!-- <div class="right-right"> -->
                                      <a href="#"><i class="fa fa-angle-left"></i></a>
                                      <a href="#"><i class="fa fa-angle-left fa-rotate-180"></i></a>
                                    <!-- </div> -->
                                </div>
                              </div>
                              
                            </div>
                          </th>
                        </tr>
                        <tr>
                          <th>
                              <div class="tab-pane fade show active" id="pills-home" role="tabpanel" aria-labelledby="pills-home-tab">There are no results matching your criteria.</div>
                              <div class="tab-pane fade" id="pills-profile" role="tabpanel" aria-labelledby="pills-profile-tab">There are no results matching .</div>
                              <div class="tab-pane fade" id="pills-contact" role="tabpanel" aria-labelledby="pills-contact-tab">There are no results.</div>
                          </th>
                        </tr>
                        <tr>
                          <th style="position: relative;">
                            <div class="row">
                            <div class="d-none d-md-block offset-8 col-md-4 right" style="position: relative; float: right;">
                              <!-- <div class="right-left"> -->
                                <span>Results per page &nbsp;</span>
                                <select ng-model="options.size" ng-change="updatePagination(options.size)" ng-options="dLimit as dLimit for dLimit in paginationLimits" class="ng-pristine ng-untouched ng-valid">
                                  <option label="10" value="number:10">10</option>
                                  <option label="20" value="number:20" selected="selected">20</option>
                                  <option label="30" value="number:30">30</option>
                                  <option label="40" value="number:40">40</option>
                                  <option label="50" value="number:50">50</option>
                                </select>
                              <!-- </div> -->
                              <!-- <div class="right-right"> -->
                                <a href="#"><i class="fa fa-angle-left"></i></a>
                                <a href="#"><i class="fa fa-angle-left fa-rotate-180"></i></a>
                              <!-- </div> -->
                          </div>
                        </div>
                          </th>
                        </tr>
                      </thead>
                    </table>
                  </div>
                </div>
              </div>
            </div>
        </section>
    </div>

    <!--<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>-->
    <!-- <script src="login.js"></script> -->
</body>
</html>