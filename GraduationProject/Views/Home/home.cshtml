        <style>
            body {
                background-color: #f5f5f5;
            }

            .card-row {
                display: flex;
                justify-content: space-between;
                flex-direction: row;
                flex-wrap: wrap;
                background-color: white;
            }

            .mt-50 {
                margin-top: 50px
            }

            .mb-50 {
                margin-bottom: 50px
            }

            .card:hover {
                z-index: 2;
                transform: scale(1.1);
                transition: 1s;
            }

            .card {

                position: relative;
                display: -ms-flexbox;
                display: flex;
                -ms-flex-direction: column;
                flex-direction: column;
                min-width: 0;
                word-wrap: break-word;
                background-color: #fff;
                background-clip: border-box;
                border: none;
                border-radius: .1875rem;
                padding: 8px;
            }

            .card-img-actions {
                position: relative
            }

            .card-body {

                padding: 1.25rem;
                text-align: center
            }

            .card-img {
                width: 100%;
                height: 6rem;
            }

            .star {
                color: rgb(255, 166, 0)
            }

            .bg-cart {
                background-color: orange;
                color: #fff
            }

            .bg-cart:hover {
                color: #fff
            }

            .bg-buy {
                background-color: green;
                color: #fff;
                padding-right: 29px
            }

            .bg-buy:hover {
                color: #fff
            }

            a {
                text-decoration: none !important
            }

            .remove-m-p {
                padding: 0;
                margin: 0;
            }

            .remove-p {
                padding: 0;
            }

            .remove-m {
                margin: 0;
            }

            .card-container {
                padding: 0 8px 8px 8px;
            }

            .card-container:hover {
                border: 2px solid rgba(0, 0, 0, .125);
            }

            .card-row-container {
                background-color: white;
            }

            .card-row-title {
                width: 100%;
                font-size: 1.5rem;
                padding: 12px;
                margin: 0;
                background-color: #FFD900;
            }

        </style>

